#include<bits/stdc++.h>

int cnt =0;
using namespace std;
vector<char>as;
char fact(string a){
    static int n=0;
if(a[n]=='\0'){

}
else{
    int x=n;
     n++;
     fact(a);
    as.push_back(a[x]);
}
}
int main(){
long long b;
string s;
getline(cin,s);
fact(s);
for(int i=0;i<as.size();i++){cout<<as[i];}
}
